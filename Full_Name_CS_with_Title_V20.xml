	outputMname = inputMname_masked.toLowerCase(); //lowercase
	outputLname = inputLname_masked.toLowerCase(); //lowercase
}
// assume Mixed case is used in lookup file
else	
{
	
	outputFname = inputFname_masked; //Sentencecase
	outputFname2 = inputFname2_masked; //Sentencecase
	outputMname = inputMname_masked; //Sentencecase
	outputLname = inputLname_masked; //Sentencecase
}


// Concatatenating fnames and lnames based on comma
if (commaFlag==1)    
{
	outputValue = outputLname + ",";
	if (trim(outputTitle).length) {
		outputValue = outputValue + " " + outputTitle;
	}
	if (trim(outputFname).length) {
		outputValue = outputValue + " " + outputFname;
