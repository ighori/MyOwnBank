	}
}

//truncate  outputValue to maximum outputLength
outputValue = trim(outputValue.substr(0,outputLength));

// if output value is empty, make sure it is set to one space character (PeopleSoft)
outputValue =  trim(outputValue).length==0?" ":outputValue;

